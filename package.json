{
  "name": "skribbl-clone",
  "version": "1.0.0",
  "description": "A clone of skribbl.io with no player limits",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "deploy:appengine": "gcloud app deploy",
    "deploy:cloudrun": "gcloud run deploy skribbl-clone --source . --region us-central1 --allow-unauthenticated --port 3000",
    "build:docker": "docker build -t skribbl-clone .",
    "run:docker": "docker run -p 3000:3000 skribbl-clone"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "keywords": ["game", "drawing", "multiplayer", "real-time"],
  "author": "Your Name",
  "license": "MIT"
}
